{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-console": "warn",
        "no-unused-vars": "warn",
        "object-curly-newline": "off"
    },
    "overrides": [
        {
            "files": [
                "./src/server.js"
            ],
            "rules": {
                "no-console": "off"
            }
        }
    ]
}